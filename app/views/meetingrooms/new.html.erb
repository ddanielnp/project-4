<h1>new meetingroom</h1>

<%= link_to "See all Meeting rooms", meetingrooms_path %>

<div class="container">

  <%= form_for @new_meeting do |f| %>

  <div class="row">
    <div class='col-sm-6'>
      <div class="form-group">
        <%= f.label :Room_title %>
        :
        <%= f.text_field :room_title %>
      </div>
    </div>
  </div>

  <div class="row">
    <div class='col-sm-6'>
      <div class="form-group">
        <%= f.label :occupancy %>
        :
        <%= f.text_field :pax %>
      </div>
    </div>
  </div>

  <div class="actions">
    <%= f.submit "Create now" %>
  </div>

  <% end %>
</div>
